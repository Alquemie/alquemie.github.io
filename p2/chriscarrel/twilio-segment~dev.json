{
    "packages": {
        "chriscarrel/twilio-segment": [
            {
                "name": "chriscarrel/twilio-segment",
                "version": "dev-main",
                "version_normalized": "dev-main",
                "source": {
                    "type": "git",
                    "url": "git@github.com:chriscarrel/twilio-segment.git",
                    "reference": "ab8d7c973fa26d3ea313fa9397c79962c88a2b0c"
                },
                "dist": {
                    "type": "zip",
                    "url": "https://api.github.com/repos/chriscarrel/twilio-segment/zipball/ab8d7c973fa26d3ea313fa9397c79962c88a2b0c",
                    "reference": "ab8d7c973fa26d3ea313fa9397c79962c88a2b0c",
                    "shasum": ""
                },
                "require": {
                    "composer/installers": "^1.0",
                    "php": "^5.6|^7"
                },
                "require-dev": {
                    "brain/monkey": "^2.2",
                    "dealerdirect/phpcodesniffer-composer-installer": "^0.4.4",
                    "phpunit/phpunit": "~5.7.9",
                    "roave/security-advisories": "dev-master",
                    "sirbrillig/phpcs-variable-analysis": "^2.0",
                    "squizlabs/php_codesniffer": "^3.2",
                    "wimg/php-compatibility": "^8.0",
                    "wp-coding-standards/wpcs": "^0.14.1"
                },
                "time": "2022-06-08T02:44:24+00:00",
                "default-branch": true,
                "type": "wordpress-plugin",
                "autoload": {
                    "exclude-from-classmap": [
                        "/tests/"
                    ]
                },
                "autoload-dev": {
                    "psr-4": {
                        "Twilio\\Segment\\Tests\\PHP\\": "tests/php/",
                        "Twilio\\Segment\\Tests\\PHP\\Unit\\": "tests/php/unit/",
                        "Twilio\\Segment\\Tests\\PHP\\Integration\\": "tests/php/integration/"
                    }
                },
                "scripts": {
                    "install-codestandards": [
                        "Dealerdirect\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin::run"
                    ],
                    "phpcs-src": [
                        "\"vendor/bin/phpcs\" src/"
                    ],
                    "phpcs-tests": [
                        "\"vendor/bin/phpcs\" tests/php/"
                    ],
                    "run-phpcs": [
                        "@phpcs-src",
                        "@phpcs-tests"
                    ],
                    "test-unit": [
                        "\"vendor/bin/phpunit\" --testsuite unit --color=always"
                    ],
                    "test-integration": [
                        "\"vendor/bin/phpunit\" --testsuite integration --configuration tests/php/integration/phpunit.xml.dist --color=always"
                    ],
                    "run-tests": [
                        "@test-unit",
                        "@test-integration"
                    ]
                },
                "license": [
                    "GPL-3.0-or-later"
                ],
                "description": "WordPress implementation of Segment analytics.js source with basic support for Gravity Forms.",
                "homepage": "https://segment.com/",
                "support": {
                    "issues": "https://github.com/chriscarrel/twilio-segment/issues",
                    "source": "https://github.com/chriscarrel/twilio-segment"
                }
            }
        ]
    }
}
